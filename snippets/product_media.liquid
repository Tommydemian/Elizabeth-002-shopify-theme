{% case media.media_type %}
    {% when 'image' %}
        <img 
        src="{{ media | image_url: width: 200, height: 200  }}"
        width="{{ media.width }}"
        height="{{ media.height }}"
        alt="{{ media.preview_image.alt }}"
        style="width: 100%; object-fit: cover;" />
    {% else %}
        
{% endcase %}